<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.dalvik.elementosui">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ElementosUI"
        tools:targetApi="31">
        <activity
            android:name=".exampleRecycler.RecyclerViewActivity"
            android:exported="false" />
        <activity
            android:name=".exampleCardView.CardViewActivity"
            android:exported="false" />
        <activity
            android:name=".MenuActivity"
            android:exported="false" />
        <activity
            android:name=".SimpleListView"
            android:exported="false" />
        <activity
            android:name=".SpinnerActivity"
            android:exported="false" />
        <activity
            android:name=".Switch"
            android:exported="false" />
        <activity
            android:name=".RadioButtons"
            android:exported="false" />
        <activity
            android:name=".CheckBox"
            android:exported="false" />
        <activity
            android:name=".Buttoon"
            android:exported="false" />
        <activity
            android:name=".ImageView"
            android:exported="false" />
        <activity
            android:name=".Edittext"
            android:exported="false" />
        <activity
            android:name=".Textview"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>